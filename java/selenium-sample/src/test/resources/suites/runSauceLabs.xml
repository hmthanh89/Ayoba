<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="utils.config.TestListener" />
	</listeners>

	<test name="Run Chrome Saucelabs">
		<parameter name="browser" value="chrome.saucelabs" />
		<parameter name="autoLogBug" value="false" />
		<classes>
			<class name="tests.JIRA.CreateIssues" />
		</classes>
	</test>
	
	<test name="Run IE Saucelabs">
		<parameter name="browser" value="ie.saucelabs" />
		<parameter name="autoLogBug" value="false" />
		<classes>
			<class name="tests.JIRA.SearchIssues" />
		</classes>
	</test>
	
</suite>