<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="5" data-provider-thread-count="5"
	parallel="methods">
	<listeners>
		<listener class-name="utils.config.TestListener" />
	</listeners>

	<test name="Run Chrome">
		<parameter name="browser" value="chrome.local" />
		<parameter name="autoLogBug" value="true" />
		<classes>
			<class name="tests.web.CreateIssueTest">
				<methods>
					<include name="TC_01_Create_An_Issue" />
				</methods>
			</class>
		</classes>
	</test>

</suite>